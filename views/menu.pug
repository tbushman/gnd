- var keys = ["tools", "info", "substrates", "filling"]

div.grid-header.mainHeader
	//if (drawtype)
	
	if (drawtype)
		a.mainmenu(href="#", title="Save", id="save")
			img.i(src="/images/icons/check.svg")
		each ing, j in keys
			each cat, i in contentData[ing]
				if (cat.unlocked && cat.index === contentData.level && drawtype === ing)
					a.tool(href="#", onclick="document.getElementById('form_"+drawtype+"_"+i+"').submit();")
						.ui
							img(src="/images/icons/"+drawtype+"_"+i+".svg")
						span.tb-font-l #{cat.name}
						if (cat.unlocked)
							form(id="form_"+drawtype+"_"+i+"", method="POST", enctype="multipart/form-data", action="/api/selectlayer/"+menuData.urltitle+"/"+menuData.pageindex+"/"+index+"/"+drawtype+"/"+i+"")
								input(type="hidden", value="1", name= cat.name)
								input(type="hidden", id="inputimg_"+drawtype+"_"+i+"", name= cat.name, value= cat.image)

	else
		a.dropdown.mainmenu(href="#", title="Menu")
			img.i(src="http://pu.bli.sh/images/menu_button.svg")

	
			
div.grid-left
	#wrapper
		div#tlmodules.dropdown
			a.module(href="/", title="home")
				.ui
					img(src="/images/icons/home.svg")
				span.tb-font-l Home
			if (menuData)
				if (menuData.publishers[0].username === loggedin)
					
						
					if (pageindex)
						a.module.dropdown(title="edit")
							.ui
								img(src="/images/icons/edit.svg")
							span.tb-font-l Edit

							each key, j in keys
								
								each ing, k in contentData[key]
									if (ing.unlocked)
										a.module.focus.drop(href="#", onclick="document.getElementById('form_"+key+"_"+k+"').submit();")
											.ui
												img(src=(ing.unlocked ? "/images/icons/"+key+"_"+k+".svg" : "/images/icons/unknown.svg"))

									
											//each ing, k in module
											span.tb-font-l #{ing.name}
											form(id="form_"+key+"_"+k+"", method="POST", enctype="multipart/form-data", action="/api/selectlayer/"+menuData.urltitle+"/"+menuData.pageindex+"/"+index+"/"+key+"/"+k+"")
												input(type="hidden", value="1", name= ing.name)
												input(type="hidden", id="inputimg_"+keys[j]+"_"+k+"", name= ing.name, value= ing.image)
									
				if (loggedin)
					a.module(href="/logout", title="logout")
						.ui
							img(src="/images/icons/logout.svg")
						span.tb-font-l Log out
			else
				a.module(href="/login", title="login") 
					.ui
						img(src="/images/icons/login.svg")
					span.tb-font-l Login
				
				//-
					a.module(href="/register", title="register") 
						.ui
							i.fa.fa-id-card-o
						span.tb-font-l Register
